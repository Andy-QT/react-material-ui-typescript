{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "functions/**/*.js",
      "functions-src/**/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "no-debugger": false,
    "no-implicit-this": true
  }
}
